    "jwt-decode": "^2.2.0",
    "material-design-lite": "^1.3.0",
    "material-ui": "^0.18.3",
    "material-ui-password-field": "^1.2.0",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-alert": "^2.1.2",
    "react-dom": "^15.5.4",
    "react-js-pagination": "^2.1.0",
    "react-notification-system": "^0.2.14",
