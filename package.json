{
  "name": "dms",
  "version": "1.0.0",
  "description": "a document managemt system",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src",
    "open:src": "nodemon ./bin/www --exec babel-node ",
    "postinstall": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "lint": "node_modules/.bin/esw webpack.config.* client server server.*",
    "lint:watch": "npm run lint -- --watch",
    "migrate": "export NODE_ENV=staging && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "export NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && babel-node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha  -R spec ./server/test/*.spec.js",
    "coverage": "./node_modules/.bin/coveralls < coverage/lcov.info",
    "test:frontend": "jest ./client/test/*.spec.js",
    "test:e2e": "istanbul cover ./node_modules/.bin/_mocha ./client/test/e2eTests/*.spec.js"
  },
  "jest": {
    "rootDir": "./",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "globals": {
      "window": true
    },
    "setupTestFrameworkScriptFile": "mock-local-storage",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverall",
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ewanjiru/document-management-system.git"
  },
  "author": "eunice",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ewanjiru/document-management-system/issues"
  },
  "homepage": "https://github.com/Ewanjiru/document-management-system#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chromedriver": "^2.30.1",
    "cookie-parser": "^1.4.3",
